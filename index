  <script>
    function validateCaptcha() {
      const userInput = document.getElementById("captchaInput").value.trim();
      if (userInput === "UR46KT") {
        window.location.href = "result.html";
      } else {
        alert("Invalid CAPTCHA. Please try again.");
      }
    }
  </script>
